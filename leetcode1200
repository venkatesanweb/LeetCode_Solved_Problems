#include <bits/stdc++.h>
using namespace std;

class Solution {
public:
    vector<vector<int>> minimumAbsDifference(vector<int>& A) {
        sort(A.begin(), A.end());
        int minDiff = INT_MAX;
        vector<vector<int>> res;

        for (int i = 1; i < A.size(); i++) {
            int diff = A[i] - A[i - 1];
            if (diff < minDiff) {
                minDiff = diff;
                res.clear();
                res.push_back({A[i - 1], A[i]});
            } else if (diff == minDiff) {
                res.push_back({A[i - 1], A[i]});
            }
        }
        return res;
    }
};

int main() {
    int n;
    cin >> n;
    vector<int> A(n);
    for (int i = 0; i < n; i++) cin >> A[i];

    Solution obj;
    vector<vector<int>> ans = obj.minimumAbsDifference(A);

    for (auto &p : ans) {
        cout << p[0] << " " << p[1] << endl;
    }
    return 0;
}

auto init = atexit([](){ ofstream("display_runtime.txt") << '0'; });
