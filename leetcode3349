#include <bits/stdc++.h>
using namespace std;

class Solution {
public:
    static vector<int> constructTransformedArray(vector<int>& nums) {
        int n = nums.size();
        vector<int> ans(n);

        for (int i = 0; i < n; i++) {
            int j = (i + nums[i]) % n;
            ans[i] = nums[j + (-(j < 0) & n)];
        }
        return ans;
    }
};

int main() {
    int n;
    cin >> n;

    vector<int> nums(n);
    for (int i = 0; i < n; i++) {
        cin >> nums[i];
    }

    vector<int> result = Solution::constructTransformedArray(nums);

    for (int x : result) {
        cout << x << " ";
    }

    return 0;
}
