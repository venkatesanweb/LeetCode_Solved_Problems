#include <bits/stdc++.h>
using namespace std;

class Solution {
public:
    int minPartitions(string n) {
        int ans = 0;

        for (int i = 0; i < n.length(); i++) {
            if (n[i] - '0' > ans)
                ans = n[i] - '0';
        }
        return ans;
    }
};

int main() {
    string n;
    cin >> n;

    Solution obj;
    int result = obj.minPartitions(n);

    cout << result << endl;
    return 0;
}
