#include <bits/stdc++.h>
using namespace std;

class Solution {
public:
    char nextGreatestLetter(vector<char>& letters, char target) {
        char res = letters[0];
        bool flag = false;

        for (char ch : letters) {
            if (!flag) {
                if (ch > target) {
                    res = ch;
                    flag = !flag;
                }
            } else {
                if (ch > target && ch < res)
                    res = ch;
            }
        }
        return res;
    }
};

int main() {
    Solution obj;

    vector<char> letters = {'c', 'f', 'j'};
    char target = 'd';

    char ans = obj.nextGreatestLetter(letters, target);
    cout << ans << endl;

    return 0;
}
