#include <bits/stdc++.h>
using namespace std;

class Solution {
public:
    vector<int> minBitwiseArray(vector<int>& nums) {
        int n = nums.size();
        vector<int> ans(n, -1);

        for (int i = 0; i < n; i++) {
            int x = nums[i];

            if ((x & 1) == 0) {
                ans[i] = -1;
                continue;
            }

            int z = ((x + 1) & ~x) >> 1;
            ans[i] = x & ~z;
        }
        return ans;
    }
};

int main() {
    int n;
    cin >> n;

    vector<int> nums(n);
    for (int i = 0; i < n; i++) {
        cin >> nums[i];
    }

    Solution sol;
    vector<int> result = sol.minBitwiseArray(nums);

    for (int x : result) {
        cout << x << " ";
    }
    cout << endl;

    return 0;
}
