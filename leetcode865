import java.util.*;

public class Main {

    static class TreeNode {
        int val;
        TreeNode left;
        TreeNode right;

        TreeNode() {}
        TreeNode(int val) { this.val = val; }
        TreeNode(int val, TreeNode left, TreeNode right) {
            this.val = val;
            this.left = left;
            this.right = right;
        }
    }

    static class Result {
        int depth;
        TreeNode node;

        Result(int depth, TreeNode node) {
            this.depth = depth;
            this.node = node;
        }
    }

    static class Solution {

        public TreeNode subtreeWithAllDeepest(TreeNode root) {
            return deep(root).node;
        }

        private Result deep(TreeNode root) {
            if (root == null)
                return new Result(0, null);

            Result left = deep(root.left);
            Result right = deep(root.right);

            if (left.depth == right.depth)
                return new Result(left.depth + 1, root);
            else if (left.depth > right.depth)
                return new Result(left.depth + 1, left.node);
            else
                return new Result(right.depth + 1, right.node);
        }
    }

    public static void main(String[] args) {

        TreeNode root = new TreeNode(3);
        root.left = new TreeNode(5);
        root.right = new TreeNode(1);
        root.left.left = new TreeNode(6);
        root.left.right = new TreeNode(2);
        root.left.right.left = new TreeNode(7);
        root.left.right.right = new TreeNode(4);
        root.right.right = new TreeNode(8);

        Solution sol = new Solution();
        TreeNode ans = sol.subtreeWithAllDeepest(root);

        System.out.println(ans.val);
    }
}
